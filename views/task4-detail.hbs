<!--contain-->
<div class="container">
  <h1 class="text-center">Aquarius Characteristics</h1>
  <div class="container-fluid well span6">
    <div class="row">
      <div class="col-md-4">
        <img src="/images{{target.imagePath}}" class="img-circle" />
      </div>

      <div class="col-md-8">
        <h3>{{target.name}}</h3>
        <h4>
          Strengths:
          <small>{{target.strengths}}</small>
        </h4>
        <h4>
          Weaknesses:
          <small>{{target.weaknesses}}</small>
        </h4>
        <h4>
          Likes:
          <small>{{target.likes}}</small>
        </h4>
        <h4>
          Dislikes:
          <small>{{target.dislikes}}</small>
        </h4>
        <h4>
          <small>{{target.description}}</small>
        </h4>
      </div>
    </div>
  </div>
</div>

<!--/contain-->